{
  "name": "reify",
  "version": "1.0.0",
  "description": "An opinionated boilerplate for client-only React apps",
  "keywords": [
    "react",
    "redux",
    "immutable",
    "ava",
    "axios",
    "redux-saga"
  ],
  "main": "src/index.js",
  "author": "Carlo DiCelico <carlodicelico@me.com> (https://github.com/carlodicelico)",
  "homepage": "https://github.com/carlodicelico/reify",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlodicelico/reify/issues"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "lru-memoize": "^1.0.1",
    "multireducer": "^3.0.0",
    "raven-js": "^3.2.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-form": "^6.0.0-rc.4",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.11.0",
    "redux-storage": "^4.0.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.11.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^6.0.0-beta.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nock": "^8.0.0",
    "nyc": "^8.1.0",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "redux-ava": "^2.0.0",
    "redux-test-recorder": "^1.0.0",
    "rimraf": "^2.5.3",
    "sass": "^0.5.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "webpack": "^1.13.1",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --colors --history-api-fallback --progress --inline --watch --hot --quiet",
    "test": "ava --no-cache",
    "test:watch": "npm test -- --watch",
    "test:verbose": "npm test -- --verbose",
    "test:werbose": "npm test -- --watch --verbose"
  },
  "ava": {
    "files": [
      "./src/**/tests/*.test.js"
    ],
    "source": [
      "./src/**/*.js",
      "!./dist/*"
    ],
    "failFast": true,
    "tap": false,
    "require": [
      "babel-register",
      "./src/utils/testingSetup.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carlodicelico/reify"
  }
}
